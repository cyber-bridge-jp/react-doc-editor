{
  "name": "react-doc-editor",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepublishOnly": "npm run build"
  },
  "main": "./dist/react-doc-editor.umd.js",
  "module": "./dist/react-doc-editor.es.js",
  "files": [
    "dist"
  ],
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/react-doc-editor.es.js",
      "require": "./dist/react-doc-editor.umd.js",
      "types": "./dist/types/index.d.ts"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cyber-bridge-jp/react-doc-editor.git"
  },
  "author": "Aizhigit Orozmamatov",
  "license": "MIT",
  "dependencies": {
    "@excalidraw/excalidraw": "0.18.0",
    "@lexical/clipboard": "0.20.0",
    "@lexical/code": "0.20.0",
    "@lexical/file": "0.20.0",
    "@lexical/hashtag": "0.20.0",
    "@lexical/html": "0.20.0",
    "@lexical/link": "0.20.0",
    "@lexical/list": "0.20.0",
    "@lexical/mark": "0.20.0",
    "@lexical/overflow": "0.20.0",
    "@lexical/plain-text": "0.20.0",
    "@lexical/react": "0.20.0",
    "@lexical/rich-text": "0.20.0",
    "@lexical/selection": "0.20.0",
    "@lexical/table": "0.20.0",
    "@lexical/utils": "0.20.0",
    "katex": "^0.16.9",
    "lodash-es": "^4.17.21",
    "lexical": "0.20.0"
  },
  "peerDependencies": {
    "react": ">=17.x",
    "react-dom": ">=17.x"
  },
  "devDependencies": {
    "@types/katex": "^0.16.7",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.11.24",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react": "^4.3.3",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "vite-plugin-dts": "^3.7.3"
  }
}
